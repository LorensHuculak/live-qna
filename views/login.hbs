    <input name="username" id="inputGroup1_1" class="form-control form-control-md rounded-0" type="text" placeholder="Enter Name...">

        </div>
      
  <div class="form-group g-mb-20">
    <input name="password" id="inputGroup1_1" class="form-control form-control-md rounded-0" type="text" placeholder="Enter Password...">

  </div>
  <!-- End Text Input -->
  
  


  
<button type="submit" class="btn u-btn-outline-teal g-mr-10 g-mt-20 rounded-0">Let's Go</button>



    
</form>


                      
